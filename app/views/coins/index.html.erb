<p style="color: green"><%= notice %></p>

<h1><%= Coin.model_name.human(count: 2) %></h1>

<p><strong>Data:</strong> <%= data_br(Date.today) %></p>

<div id="coins">
  <% @coins.each do |coin| %>
    <%= render coin %>
    <p>
      <%= link_to t('links.show'), coin %>
    </p>
  <% end %>
</div>

<%= link_to "New coin", new_coin_path %>

<div>
  <small><%= application_name %></small>
</div>
